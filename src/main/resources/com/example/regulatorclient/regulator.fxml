<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.regulatorclient.TemperatureController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <TableView fx:id="tableView" layoutX="20.0" layoutY="14.0" prefHeight="200.0" prefWidth="233.0">
              <columns>
                <TableColumn fx:id="temperatureColumn" prefWidth="232.0" text=" Значения температуры" />
              </columns>
            </TableView>
            <Button layoutX="290.0" layoutY="60.0" mnemonicParsing="false" onAction="#insertTemperatureButtonClick" text="Задать температуру" />
            <TextField layoutX="281.0" layoutY="14.0" fx:id="textField" />
            <Button layoutX="72.0" layoutY="221.0" mnemonicParsing="false" onAction="#getListButtonClick" text="Получить данные" />
      </children>
    </AnchorPane>
  </children>
</VBox>
